<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>MindboxHeadHunterTask-WSW</PackageId>
    <Title>MindboxHeadHunterTask.WSW</Title>
    <Copyright>MIT</Copyright>
    <Description>Собственно, пакетик для калькуляции площади фигуры</Description>
    <PackageProjectUrl>https://github.com/xWSWx</PackageProjectUrl>
    <PackageOutputPath>$(SolutionDir)</PackageOutputPath>
    <PackageReadmeFile>ReadMe.MD</PackageReadmeFile>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="ReadMe.MD">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
